###
# Makefile.in for mod_auth_kerb
###

APXS = @APXS@
CFLAGS = -I. @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@

all: mod_auth_kerb

mod_auth_kerb: mod_auth_kerb.c
	$(APXS) -c ${CFLAGS} ${LDFLAGS} ${LIBS} mod_auth_kerb.c

install:
	$(APXS) -c -i ${CFLAGS} ${LDFLAGS} ${LIBS} mod_auth_kerb.c

clean:
	/bin/rm -rf core *.o *.la *.lo *.slo *.so .libs

distclean: clean
	/bin/rm -f Makefile
