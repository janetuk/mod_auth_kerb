###
# Makefile.in for mod_auth_kerb
###

APXS = @APXS@
CFLAGS = -I. @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
SUBDIRS = src
MAKE_SUBDIR = for dir in $(SUBDIRS) ; do ( cd $$dir ; $(MAKE) $@ ) ; done

all:
	@$(MAKE_SUBDIR)

install:
	@$(MAKE_SUBDIR)

clean:
	@$(MAKE_SUBDIR)

distclean: clean
	@$(MAKE_SUBDIR)
	/bin/rm -f Makefile config.h config.status config.cache config.log
